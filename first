#include<stdio.h>
int broj(int m)
{
    if(m==0) return 0; 
    int r=broj(m/10);
    int c=m%10;
    if(c<5)
    {printf("%d", c);
    r++;
    }

return r;

}
int main()
{
    int n,i, m;
    scanf("%d", &n);
    for(i=0; i<n; i++)
    {
    scanf("%d", &m);
        int k=broj(m);
        printf(" : %d\n", k);
    
    }
    return 0;
}
